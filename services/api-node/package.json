{
  "name": "@susukonnect/api-node",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "predev": "npm --prefix ../../packages/shared run build",
    "dev": "tsx watch src/server.ts",
    "prebuild": "npm --prefix ../../packages/shared run build",
    "build": "tsc -p tsconfig.json",
    "prestart": "npm --prefix ../../packages/shared run build",
    "start": "node dist/server.js",
    "lint": "tsc -p tsconfig.json --noEmit",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pretest": "npm --prefix ../../packages/shared run build",
    "test": "vitest run"
  },
  "dependencies": {
    "@susukonnect/shared": "*",
    "axios": "latest",
    "bcryptjs": "latest",
    "cors": "latest",
    "dotenv": "latest",
    "express": "latest",
    "helmet": "latest",
    "jsonwebtoken": "latest",
    "morgan": "latest",
    "stripe": "latest",
    "uuid": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@types/bcryptjs": "latest",
    "@types/cors": "latest",
    "@types/express": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/morgan": "latest",
    "@types/node": "latest",
    "@types/supertest": "latest",
    "supertest": "latest",
    "tsx": "latest",
    "typescript": "latest",
    "vitest": "latest"
  }
}
